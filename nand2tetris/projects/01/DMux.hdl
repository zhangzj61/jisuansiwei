/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here:
    Xor(a=in,b=in,out=bb);
    Or(a=in,b=bb,out=aa);
    Mux(a=aa,b=bb,sel=sel,out=a);
    Mux(a=bb,b=aa,sel=sel,out=b);
}
