/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Xor(a=a,b=b,out=xor);
    And(a=xor,b=sel,out=flag);
    Xor(a=a,b=flag,out=out);
}